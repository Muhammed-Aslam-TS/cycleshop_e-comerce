                                                data-zoom-image="/productMulterImage/{{products.img.[2]}}">
                                                <img src="/productMulterImage/{{products.img.[2]}}"
                                                    alt="product with model">
                                            </a>

                                            <a class="product-gallery-item" href="#"
                                                data-image="/productMulterImage/{{products.img.[3]}}"
                                                data-zoom-image="/productMulterImage/{{products.img.[3]}}">
                                                <img src="/productMulterImage/{{products.img.[3]}}" alt="product back">
                                            </a>
                                        </div><!-- End .product-image-gallery -->
                                    </div><!-- End .row -->
                                </div><!-- End .product-gallery -->
                            </div><!-- End .col-md-6 -->

                            <div class="col-md-6">
                                <div class="product-details">
                                    <h1 class="product-title">{{products.Name}}</h1><!-- End .product-title -->

                                    <div class="ratings-container">
                                        <div class="ratings">
                                            <div class="ratings-val" style="width: 80%;"></div><!-- End .ratings-val -->
                                        </div><!-- End .ratings -->
                                        <a class="ratings-text" href="#product-review-link" id="review-link">( 2 Reviews
                                            )</a>
