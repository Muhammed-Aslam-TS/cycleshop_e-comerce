                                    <div class="product-content">
                                        <p>{{products.Description}} </p>
                                    </div><!-- End .product-content -->





                                    <div class="product-details-action">
                                        <a href="#" class="btn-product btn-cart"
                                            onclick="addToCart('{{products._id}}','{{products.Price}}')"><span>add to
                                                cart</span></a>

