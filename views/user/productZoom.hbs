                <div class="container">
                    <div class="product-details-top">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="product-gallery product-gallery-vertical">
                                    <div class="row">
                                        <figure class="product-main-image">
                                            <img id="zoom_01" data-imagezoom="true"
                                                src="/productMulterImage/{{products.img.[0]}}"
                                                data-zoom-image="/productMulterImage/{{products.img.[0]}}"
                                                alt="product image">

                                            <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                                                <i class="icon-arrows"></i>
                                            </a>
                                        </figure><!-- End .product-main-image -->

                                        <div id="product-zoom-gallery" class="product-image-gallery">
                                            <a class="product-gallery-item active" href="#"
                                                data-image="/productMulterImage/{{products.img.[0]}}"
                                                data-zoom-image="/productMulterImage/{{products.img.[0]}}">
                                                <img src="/productMulterImage/{{products.img.[0]}}" alt="product side">
                                            </a>

                                            <a class="product-gallery-item" href="#"
                                                data-image="/productMulterImage/{{products.img.[1]}}"
                                                data-zoom-image="/productMulterImage/{{products.img.[1]}}">
