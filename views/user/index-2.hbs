                        let count = $('#wishlistCount').html()
                        count = parseInt(count) +
                            $("#wishlistCount").html(count)

                        Swal.fire(
                            'Add to wish List!',
                            'Good job',
                            'success'
                        ).then(() => {
                            location.reload()
                        })
                    }

                }

            })
        }
    </script>