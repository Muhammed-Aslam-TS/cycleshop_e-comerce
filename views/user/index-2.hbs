    <script>
        function addToCart(productId) {
            $.ajax({
                url: '/addToCart/' + productId,
                method: 'get',
                success: (response) => {
                    if (response.status) {

                        let count = $('#cartCount').html()
                        count = parseInt(count) +
                            $("#cartCount").html(count)

                        Swal.fire(
                            'Add to cart',
                            'God job',
                            'success'
                        ).then(() => {
                            location.reload()
                        })
                    }

                }

            })
        }
    </script>

    <script>
        function addToWishList(productId) {
            $.ajax({
                url: '/addToWishList/' + productId,
                method: 'get',
                success: (response) => {
